{
  "description": "Private Network Web Proxy",
  "template_type": "web-proxy",
  "instance_type": "t3.small",
  "os_name": "amazon-linux-2",
  "os_base_ami_regexp": "amzn2-ami-kernel-5.10-hvm-2.0.*-x86_64-gp2",
  "autoscaling_group": {
    "tcp_port": 80,
    "health_check_port": 80,
    "max_size": 4,
    "min_size": 1
  },
  "volume_type": "gp3",
  "volume_size": 10,
  "volume_iops": 3000,
  "volume_throughput": 250,
  "components_aws": [
    "amazon-cloudwatch-agent-linux"
  ],
  "components_custom": [
    "web-proxy"
  ],
  "policies_managed": [],
  "policies_inline": [
    {
      "Action": [
				"s3:GetObject",
				"s3:List*"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::${account_id}-${region}-${prefix}-web",
        "arn:aws:s3:::${account_id}-${region}-${prefix}-web/*"
      ],
      "Condition": {}
    }
  ]
}