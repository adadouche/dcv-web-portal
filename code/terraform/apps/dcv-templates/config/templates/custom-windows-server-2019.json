{
  "description": "a custom windows server 2019 instance in various sizes",
  "template_type": "workstation",
  "instance_type": "t3.medium",
  "instance_families": [
    "t3"
  ],
  "instance_sizes": [
    "medium",
    "large"
  ],
  "enable_hibernation" :true,
  "os_name": "windows-server-2019",
  "os_base_ami_regexp": "Windows_Server-2019-English-Full-Base-*",
  "volume_type": "gp3",
  "volume_size": 80,
  "volume_size_min": 50,
  "volume_size_max": 300,
  "volume_iops": 3000,
  "volume_throughput": 250,
  "cognito_groups": [
    "user-group-1"
  ],
  "components_aws": [
    "amazon-cloudwatch-agent-windows"
  ],
  "components_custom": [
    "custom-component-windows",
    "custom-component-windows-server",
    "custom-component-windows-server-2019"
  ],
  "policies_managed": [
    "AmazonS3FullAccess"
  ],
  "policies_inline": []
}